				$('.scroll-pane-mods').jScrollPane();			
				$('.scroll-pane-rate').jScrollPane();			
                stylesheetToggle();
                $(window).resize(stylesheetToggle);			
			});

            
            function stylesheetToggle()
            {
                if ($('body').width() <= 768 && $('body').width() >= 468) {
                    if(show_width==2 || show_width==3 || show_width==0)
                    {
                        $.ajax({
                            url: '/small.php',
                            type: 'GET',
                            dataType: 'json',
                            success: function(data, textStatus, jqXHR)
                            {
                                $(".ad_top").html(data.ad_top);
                                $(".ad_middle").html(data.ad_middle);
                                $(".ad_bottom").html(data.ad_bottom);
								
                            }
                        });
                        show_width = 1;
                    }
                }
                else if ($('body').width() < 468) {
                    if(show_width==1 || show_width==3 || show_width==0)
                    {
                        $.ajax({
                            url: '/tiny.php',
                            type: 'GET',
                            dataType: 'json',
                            success: function(data, textStatus, jqXHR)
                            {
                                $(".ad_top").html(data.ad_top);
                                $(".ad_middle").html(data.ad_middle);
                                $(".ad_bottom").html(data.ad_bottom);
								
                            }
                        });
                        show_width = 2;
                    }
                } else {
                    if(show_width==1 || show_width==2 || show_width==0)
                    {
                        $.ajax({
                            url: '/normal.php',
                            type: 'GET',
                            dataType: 'json',
                            success: function(data, textStatus, jqXHR)
                            {
                                $(".ad_top").html(data.ad_top);
                                $(".ad_middle").html(data.ad_middle);
                                $(".ad_bottom").html(data.ad_bottom);
                            }
                        });
                        show_width = 3;
                    }
                }
            }    
</script>


