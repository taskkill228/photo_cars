	function add_compare(id)
		{
			$.ajax(
			    { 
			      type: "GET",
			      url: "//drive.place/add_compare/"+id,
			      dataType: 'html',
			      xhrFields: { withCredentials: true },
			      crossDomain: true,
			      success: function(data){
			    	  if(data==-1)
						{
							$(".notice").css("display", "block");
							$('.notice').delay(1000).fadeOut();
						} else {
							$("#compare").html(data);
							$.ajax(
								    { 
								      type: "GET",
								      url: "//drive.place/add_compare2/"+id,
								      dataType: 'html',
								      crossDomain: true,
								      xhrFields: { withCredentials: true },
								      success: function(data){
								    	  $("#compare2").html(data);
								      }
								    }
							);
						}	
			      }
			    }
			);

						$('.compare_wrapper').fadeIn(600);
			return false;
	}
</script>

